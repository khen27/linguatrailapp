{
  "expo": {
    "name": "linguatrail",
    "slug": "linguatrail",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icons-ios/Icon-1024.png",
    "scheme": "linguatrail",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/app-icons-ios/Icon-1024.png",
      "buildNumber": "1",
      "supportsTablet": true,
      "bundleIdentifier": "com.karlvizi.linguatrail",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 20000,
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons-android/Icon-512.png",
        "backgroundColor": "#0E1E49"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.karlvizi.linguatrail"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/app-icons-android/Icon-512.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#091729",
          "dark": {
            "backgroundColor": "#091729"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e09af915-70e1-4b45-9408-199c39e801db"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/e09af915-70e1-4b45-9408-199c39e801db"
    }
  }
}
